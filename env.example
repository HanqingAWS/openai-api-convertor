# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# DynamoDB (optional - for local development)
DYNAMODB_ENDPOINT_URL=http://localhost:8001

# DynamoDB Tables
DYNAMODB_API_KEYS_TABLE=openai-proxy-api-keys
DYNAMODB_USAGE_TABLE=openai-proxy-usage
DYNAMODB_MODEL_MAPPING_TABLE=openai-proxy-model-mapping

# Authentication
REQUIRE_API_KEY=false
MASTER_API_KEY=test-key

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Features
ENABLE_VISION=true
ENABLE_TOOL_USE=true
ENABLE_EXTENDED_THINKING=true

# Application
DEBUG=true
LOG_LEVEL=DEBUG
